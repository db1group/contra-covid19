{
  "name": "api-notificasaude",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "pretest": "cross-env NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "posttest": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "sonar": "node sonar-project.js",
    "start": "node src/server.js",
    "lint": "npx eslint src/**/*.js",
    "dev": "cross-env NODE_ENV=development nodemon --inspect src/server.js",
    "migrate": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "build": "cross-env NODE_ENV=prod node src/server.js"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "__tests__/coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "moment": "^2.24.0",
    "pg": "^8.0.0",
    "pino-http": "^5.0.0",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "nodemon": "^2.0.3",
    "sonarqube-scanner": "^2.6.0",
    "jest": "^25.5.2",
    "jest-sonar-reporter": "^2.0.0",
    "cross-env": "^7.0.2",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "2.20.1",
    "prettier": "^2.0.4"
  }
}